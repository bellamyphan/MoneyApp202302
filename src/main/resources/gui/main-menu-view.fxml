<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Button?>

<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml" fx:controller="gui.MainMenuController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <Button text="Transaction Menu" fx:id="transactionMenuButton" onAction="#transactionMenuOnAction"/>
    <Button text="Bank Menu" fx:id="bankMenuButton" onAction="#bankMenuOnAction"/>
    <Button text="Report Menu" onAction="#reportMenuOnAction"/>
    <Button text="Export Menu" onAction="#exportMenuOnAction"/>
    <Button text="Investment and Saving Menu" onAction="#investmentAndSavingMenuOnAction"/>
    <Button text="Net Worth Menu" onAction="#netWorthMenuOnAction"/>
    <Button text="Exit this menu" fx:id="exitThisMenuButton" onAction="#exitThisMenuOnAction"/>
</VBox>
